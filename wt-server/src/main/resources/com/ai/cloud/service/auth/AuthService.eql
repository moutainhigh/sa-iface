--[recordUserVisitTime]
UPDATE TF_F_USER
   SET LAST_VISIT_TIME = now()
 WHERE UID = #uid#
   AND PHONE_NO = #phoneNo#

--[querySupportVersion]
SELECT SUPPORT_VERSION "supportTo",CASE WHEN PROVINCE_CODE ='98' THEN '999' ELSE PROVINCE_CODE END SORT_NUM
    FROM TF_F_APP_VERSION_MIN_REQUIRED
    WHERE SUIT_TYPE=#1#
      AND PROVINCE_CODE IN ('98', #2#)
      AND CITY_CODE IN ('000', #3#)
      ORDER BY SORT_NUM ASC, CITY_CODE DESC